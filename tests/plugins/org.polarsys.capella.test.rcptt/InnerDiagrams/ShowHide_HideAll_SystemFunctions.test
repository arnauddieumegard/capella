------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Capella Project Explorer" | get-tree
    | select "MultipleFunctions/MultipleFunctions.aird" | double-click
    | select "MultipleFunctions/MultipleFunctions.aird/MultipleFunctions/System Analysis/System Functions/Root System Function/[SDFB] Root System Function1" 
    | double-click

with [get-editor "[SDFB] Root System Function1"] {
    get-diagram -index 1 | get-edit-part -name "[SDFB] Root System Function1" | mouse-move 2232 415 -height 1060 
        -width 2236
    with [get-palette] {
        get-palette-entry "Functional Chains" | mouse-move 37 0 -height 57 -width 125
        get-palette-entry "Functional Exchanges" -index 1 | mouse-move 80 0 -height 57 -width 125
        with [get-palette-entry Functions -index 1] {
            mouse-move 75 22 button1 -height 32 -width 125
            mouse-press 75 22 button1 -height 32 -width 125
            mouse-release 75 22 button1 524288 -height 32 -width 125
        }
    }
    with [get-diagram -index 1 | get-edit-part -name "[SDFB] Root System Function1"] {
        mouse-move 714 285 button1 -height 1060 -width 2236
        mouse-press 714 285 button1 -height 1060 -width 2236
        mouse-release 714 285 button1 524288 -height 1060 -width 2236
    }
}
with [get-window "Transfer Dialog"] {
    get-editbox | click-text
    get-button "<<" | click
    get-button OK | click
}

get-editor "[SDFB] Root System Function1" | get-diagram -index 1 | get-edit-part -name "[SDFB] Root System Function1" 
    | get-property childrenCount | equals 0 | verify-true
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
